        .product-holder
          p
            span.product-description.handwritten-font.black Våra 
            span.black.clean-font.uppercase.medium Continentalsängar 
          = image_tag "continental.png", class:"img-responsive product-image", style: "width: 350px; margin-top:100px;"
  .col-xs-12.col-md-4.col-sm-4.firts-page-container.second
    .brand= image_tag 'dreamliner-logo.png'
    
